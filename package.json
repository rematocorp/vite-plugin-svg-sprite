{
  "name": "@remato/vite-plugin-svg-sprite",
  "version": "0.3.5",
  "description": "SVG sprite plugin for Vite",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./es/index.d.ts",
        "default": "./es/index.js"
      },
      "require": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      }
    },
    "./runtime": "./es/runtime.js",
    "./typings/*": {
      "types": "./typings/*.d.ts"
    }
  },
  "files": [
    "lib",
    "es",
    "src",
    "LICENSE"
  ],
  "scripts": {
    "lint": "eslint src",
    "build": "tsc && npx tsc -m es2015 --outDir es",
    "prepublish": "npm run build"
  },
  "keywords": [
    "vite",
    "svg"
  ],
  "author": "meowtec<bertonzh@gmail.com>",
  "contributors": [
    "viljark <viljar@remato.com>"
  ],
  "homepage": "https://github.com/rematocorp/vite-plugin-svg-sprite",
  "bugs": {
    "url": "https://github.com/rematocorp/vite-plugin-svg-sprite/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/micromatch": "^4.0.1",
    "@types/node": "^14.14.21",
    "@types/svgo": "^2.6.4",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-plugin-import": "^2.22.1",
    "typescript": "^4.1.2",
    "vite": "^4.0.0"
  },
  "dependencies": {
    "micromatch": "^4.0.2",
    "svg-baker": "~1.7.0",
    "svgo": "^2.8.0"
  },
  "peerDependencies": {
    "vite": "^2 || ^3 || ^4"
  }
}
